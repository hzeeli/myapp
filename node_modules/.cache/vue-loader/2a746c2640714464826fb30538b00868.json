olor=\"#F05654\">\r\n                                <i slot=\"icon\" class=\"fa fa-heart\"></i>\r\n                            </vue-star>\r\n                        </div>\r\n                    </div>\r\n                </div>      \r\n                \r\n            </mt-tab-container-item>\r\n            <mt-tab-container-item id=\"2\">\r\n                该功能暂未开放\r\n            </mt-tab-container-item>\r\n        </mt-tab-container>\r\n    </div>\r\n</template>\r\n<script>\r\n    import Aplayer from 'vue-aplayer';\r\n    import VueStar from 'vue-star';\r\n    export default{\r\n        data(){\r\n            return {\r\n                selected: '1',\r\n                audio_list:[],\r\n            };\r\n        },\r\n        methods:{\r\n           getAudio_list(){\r\n               var url = \"http://w.rdc.sae.sina.com.cn:3306/media/audio\";\r\n                this.$http.get(url).then(result=>{\r\n                    if(result.body.code==1){\r\n                        this.audio_list=result.body.msg;\r\n                    }else{\r\n                        Toast(\"加载mp3失败\")\r\n                    }\r\n                })\r\n           }\r\n        },\r\n        created(){\r\n            this.getAudio_list()\r\n        },\r\n        computed:{\r\n            \r\n        },\r\n        components: {\r\n            Aplayer,\r\n            VueStar\r\n        }\r\n    }\r\n</script>\r\n<style>\r\n    .app_media .mint-navbar .mint-tab-item{\r\n        font-size: 30px;\r\n        color: #929292;\r\n    }\r\n    .app_media .mint-navbar .mint-tab-item.is-selected{\r\n        color: #222;\r\n        border-bottom: 2px solid #333;\r\n    }\r\n    .app_media .mui-card-content.media_content{\r\n            padding: 0px;\r\n    }\r\n    .app_media .mui-card.my-card{\r\n        padding: 0;\r\n    }\r\n    .app_media .mui-card-header{\r\n        font-size: 13px;\r\n    }\r\n    .app_media img{\r\n        width: 100%;\r\n    }\r\n    .app_media .mui-card .mui-card-footer{\r\n        margin: -20px 5px;\r\n    }\r\n</style>\r\n"]}]}