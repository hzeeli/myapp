ent() {\n      var _this3 = this;\n\n      this.pageIndex++;\n      var url = \"newslist/commentlist?nid=\" + this.id + \"&pno=\" + this.pageIndex;\n      this.$http.get(url).then(function (result) {\n        console.log(result.body.msg.data);\n\n        if (!result.body.msg.data.length == 0) {\n          _this3.list = _this3.list.concat(result.body.msg.data);\n          Toast(\"加载成功\");\n        } else {\n          Toast(\"没有更多了\");\n        }\n      });\n    }\n  },\n  created: function created() {\n    this.getCommentList();\n  },\n  props: [\"id\"] //接受父组件传递参数\n\n};",{"version":3,"sources":["comment.vue"],"names":[],"mappings":";;;;;;;;;;;;;;;;;;AAmBA,SAAA,KAAA,QAAA,SAAA;AACA,eAAA;AACA,MADA,kBACA;AACA,WAAA;AACA,YAAA,EADA;AAEA,WAAA,EAFA;AAGA,iBAAA,CAHA,CAGA;;AAHA,KAAA;AAKA,GAPA;AAQA,WAAA;AACA,eADA,yBACA;AAAA;;AACA,UAAA,WAAA,MAAA,CADA,CACA;;AACA,UAAA,MAAA,KAAA,EAAA,CAFA,CAEA;;AACA,UAAA,UAAA,KAAA,GAAA,CAHA,CAGA;;AACA,UAAA,MAAA,sBAAA,CAJA,CAIA;AACA;;AACA,UAAA,MAAA;AAAA,aAAA,GAAA;AAAA,iBAAA,OAAA;AAAA,kBAAA;AAAA,OAAA;AACA,WAAA,KAAA,CAAA,IAAA,CAAA,GAAA,EAAA,GAAA,EAAA,IAAA,CAAA,kBAAA;AACA,cAAA,OAAA,IAAA,CAAA,GAAA;AACA,cAAA,GAAA,GAAA,EAAA,CAFA,CAEA;;AACA,cAAA,cAAA,GAHA,CAGA;;AACA,OAJA;AAKA,KAbA;AAcA,kBAdA,4BAcA;AAAA;;AACA,UAAA,MAAA,8BAAA,KAAA,EAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,kBAAA;AACA,eAAA,IAAA,GAAA,OAAA,IAAA,CAAA,GAAA,CAAA,IAAA;AACA,OAFA;AAGA,KAnBA;AAoBA,kBApBA,4BAoBA;AAAA;;AACA,WAAA,SAAA;AACA,UAAA,MAAA,8BAAA,KAAA,EAAA,GAAA,OAAA,GAAA,KAAA,SAAA;AACA,WAAA,KAAA,CAAA,GAAA,CAAA,GAAA,EAAA,IAAA,CAAA,kBAAA;AACA,gBAAA,GAAA,CAAA,OAAA,IAAA,CAAA,GAAA,CAAA,IAAA;;AACA,YAAA,CAAA,OAAA,IAAA,CAAA,GAAA,CAAA,IAAA,CAAA,MAAA,IAAA,CAAA,EAAA;AACA,iBAAA,IAAA,GAAA,OAAA,IAAA,CAAA,MAAA,CAAA,OAAA,IAAA,CAAA,GAAA,CAAA,IAAA,CAAA;AACA,gBAAA,MAAA;AACA,SAHA,MAGA;AACA,gBAAA,OAAA;AACA;AACA,OARA;AASA;AAhCA,GARA;AA0CA,SA1CA,qBA0CA;AACA,SAAA,cAAA;AACA,GA5CA;AA6CA,SAAA,CAAA,IAAA,CA7CA,CA6CA;;AA7CA,CAAA","sourcesContent":["<template>\r\n    <div class=\"app_subcomment\">\r\n        <hr>\r\n        <textarea name=\"\" placeholder=\"请输入评论的内容（120字内）\" maxlength=\"120\" id=\"\" cols=\"30\" rows=\"2\" v-model=\"msg\"></textarea>\r\n        <mt-button size=\"large\" type=\"primary\" @click=\"postComment\">发表评论</mt-button>\r\n        <div class=\"cmt-list\">\r\n            <div class=\"cmt-list-item\"  v-for=\"item in list\" :key=\"item.id\">\r\n                <div class=\"cmt-title\" >\r\n                    第{{item.id}}楼&nbsp;&nbsp;用户：{{item.username}}&nbsp;&nbsp;发表时间：{{item.ctime | dateFormat}}\r\n                </div>\r\n                <div class=\"cmt-body\">\r\n                    {{item.content}}\r\n                </div>\r\n            </div>\r\n        </div>\r\n        <mt-button type=\"danger\" size=\"large\" @click=\"getMoreComment\">加载更多</mt-button>\r\n    </div>\r\n</template>\r\n<script>\r\n    import {Toast} from \"mint-ui\";\r\n    export default{\r\n        data(){\r\n            return {\r\n                list:[],\r\n                msg:\"\",\r\n                pageIndex:0,  //双向绑定的留言\r\n            };\r\n        },\r\n        methods:{\r\n            postComment(){\r\n                var username=\"匿名用户\";//用户名\r\n                var nid = this.id;      //新闻编号\r\n                var content=this.msg;            //双向绑定留言内容\r\n                var url = \"newslist/saveComment\";//设置请求路径\r\n                //console.log(nid,content)\r\n                var obj={nid:nid,content:content,username:username};  \r\n                this.$http.post(url,obj).then(result=>{\r\n                    Toast(result.body.msg);\r\n                    this.msg=\"\";//输入框清空\r\n                    this.getCommentList();//重新生成评论列表（刷新评论列表）\r\n                })\r\n            },\r\n            getCommentList(){\r\n                var url = \"newslist/commentlist?nid=\"+this.id;\r\n                this.$http.get(url).then(result=>{\r\n                    this.list=result.body.msg.data;\r\n                })\r\n            },\r\n            getMoreComment(){\r\n                this.pageIndex++;\r\n                var url = \"newslist/commentlist?nid=\"+this.id+\"&pno=\"+this.pageIndex;\r\n                this.$http.get(url).then(result=>{\r\n                    console.log(result.body.msg.data)\r\n                    if(!result.body.msg.data.length==0){\r\n                        this.list=this.list.concat(result.body.msg.data);\r\n                        Toast(\"加载成功\")\r\n                    }else{\r\n                        Toast(\"没有更多了\")\r\n                    }                 \r\n                })\r\n            }\r\n        },\r\n        created(){\r\n            this.getCommentList();\r\n        },\r\n        props:[\"id\"],   //接受父组件传递参数\r\n    }\r\n</script>\r\n<style>\r\n    .app_subcomment textarea{\r\n        font-size: 14px;\r\n    }\r\n    .app_subcomment .cmt-list{\r\n        margin:5px 0;\r\n    }\r\n    .app_subcomment .cmt-list .cmt-list-item{\r\n        font-size: 12px;\r\n    }\r\n    .app_subcomment .cmt-list .cmt-list-item .cmt-title{\r\n        line-height:30px;\r\n        background: #ccc;\r\n    }\r\n    .app_subcomment .cmt-list .cmt-list-item .cmt-body{\r\n        line-height: 30px;\r\n        text-indent: 2em;\r\n    }\r\n</style>"],"sourceRoot":"src\\components\\subcomponents"}]}